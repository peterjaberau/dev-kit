{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "jsx": "preserve",
    "declaration": true,
    "declarationMap": true,
    "moduleDetection": "force",
    "noUncheckedIndexedAccess": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/src/*": ["./*"],
      "@/*": ["./*"],
      "~/*": ["./public/*"],

      "#app/*": ["./src/app/*"],
      "#app": ["./src/app"],

      "#adaptive/*": ["./src/app/adaptive/*"],
      "#adaptive": ["./src/app/adaptive"],

      "#adaptive-components/*": ["./src/app/adaptive/components/*"],
      "#adaptive-components": ["./src/app/adaptive/components"],

      "#adaptive-core/*": ["./src/app/adaptive/core/*"],
      "#adaptive-core": ["./src/app/adaptive/core"],

      "#adaptive-registry/*": ["./src/app/adaptive/registry/*"],
      "#adaptive-registry": ["./src/app/adaptive/registry"],

      "#adaptive-shared/*": ["./src/app/adaptive/shared/*"],
      "#adaptive-shared": ["./src/app/adaptive/shared"],

      "#adaptive-modules/*": ["./src/app/adaptive/modules/*"],
      "#adaptive-modules": ["./src/app/adaptive/modules"],

      "#adaptive-dnd/*": ["./src/app/adaptive/modules/adaptive-dnd/*"],
      "#adaptive-dnd": ["./src/app/adaptive/modules/adaptive-dnd"],

      "#adaptive-json/*": ["./src/app/adaptive/modules/adaptive-json/*"],
      "#adaptive-json": ["./src/app/adaptive/modules/adaptive-json"],

      "#adaptive-tree/*": ["./src/app/adaptive/modules/adaptive-tree/*"],
      "#adaptive-tree": ["./src/app/adaptive/modules/adaptive-tree"],

      "#modules/*": ["./src/modules/*"],
      "#modules": ["./src/modules"],

      "#components/*": ["./src/components/*"],
      "#components": ["./src/components"],

      /** modules */

      "#json-tree/*": ["./src/modules/json-tree/*"],
      "#json-tree": ["./src/modules/json-tree/"],
      "#drag-and-drop/*": ["./src/modules/drag-and-drop/*"],
      "#drag-and-drop": ["./src/modules/drag-and-drop/"],

      /** experiments */

      /** possible deletion */

      "#atlas-ui/*": ["./src/components/atlas/ui/*"],
      "#atlas-ui": ["./src/components/atlas/ui/"],
      "#atlas-packages/*": ["./src/components/atlas/packages/*"],
      "#atlas-packages": ["./src/components/atlas/packages/"],

      "#shared/*": ["./src/modules/json-tree/*"],
      "#shared": ["./src/modules/json-tree/"],

      "#rui/*": ["./src/rui/*"],
      "#rui": ["./src/rui/"],

      "#actor-kit/*": ["./src/actor-kit/*"],
      "#actor-kit": ["./src/actor-kit/"],

      "#datasets/*": ["./src/datasets/*"],
      "#datasets": ["./src/datasets/"],

      "#actors/*": ["./src/actors/*"],
      "#actors": ["./src/actors/"],

      "#hooks/*": ["./src/hooks/*"],
      "#hooks": ["./src/hooks"],

      "#views/*": ["./src/views/*"],
      "#views": ["./src/views"],

      "#xflows-plugin-react/*": ["./src/orchestrator/packages/plugin-react/*"],
      "#xflows-plugin-react": ["./src/orchestrator/packages/plugin-react"],

      "#xflows-plugins/*": ["./src/orchestrator/packages/plugins/*"],
      "#xflows-plugins": ["./src/orchestrator/packages/plugins"],

      "#xflows-plugin-http/*": ["./src/orchestrator/packages/plugin-http/*"],
      "#xflows-plugin-http": ["./src/orchestrator/packages/plugin-http"],

      "#xflows-core/*": ["./src/orchestrator/packages/core/*"],
      "#xflows-core": ["./src/orchestrator/packages/core"],

      "#xflows-renderer-core/*": ["./src/orchestrator/packages/renderer-core/*"],
      "#xflows-renderer-core": ["./src/orchestrator/packages/renderer-core"],

      "#orchestrator/*": ["./src/orchestrator/*"],
      "#orchestrator": ["./src/orchestrator"],

      "#dockview/*": ["./src/modules/dockview/view/*"],
      "#dockview": ["./src/modules/dockview/view"],

      "#plugin-dock-view/*": ["./src/plugins/dock-view/*"],
      "#plugin-dock-view": ["./src/plugins/dock-view"],

      "#plugin-shared/*": ["./src/plugins/shared/*"],
      "#plugin-shared": ["./src/plugins/shared"],

      "#plugins/*": ["./src/plugins/*"],
      "#plugins": ["./src/plugins"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.cjs",
    "**/*.mjs",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
