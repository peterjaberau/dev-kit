{
  "$ref": "#/definitions/MessagePruningEvent",
  "definitions": {
    "MessagePruningEvent": {
      "type": "object",
      "properties": {
        "timestamp": {
          "type": "string",
          "format": "date-time",
          "description": "Event timestamp"
        },
        "prunedMessages": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "messageId": {
                "type": "string"
              },
              "role": {
                "type": "string",
                "enum": [
                  "system",
                  "user",
                  "assistant",
                  "function",
                  "tool"
                ]
              },
              "tokens": {
                "type": "integer",
                "minimum": 0
              },
              "importance": {
                "type": "number",
                "minimum": 0,
                "maximum": 1
              }
            },
            "required": [
              "messageId",
              "role",
              "tokens"
            ],
            "additionalProperties": false
          }
        },
        "tokensFreed": {
          "type": "integer",
          "minimum": 0
        },
        "messagesRemoved": {
          "type": "integer",
          "minimum": 0
        },
        "remainingTokens": {
          "type": "integer",
          "minimum": 0
        },
        "remainingMessages": {
          "type": "integer",
          "minimum": 0
        }
      },
      "required": [
        "timestamp",
        "prunedMessages",
        "tokensFreed",
        "messagesRemoved",
        "remainingTokens",
        "remainingMessages"
      ],
      "additionalProperties": false
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}