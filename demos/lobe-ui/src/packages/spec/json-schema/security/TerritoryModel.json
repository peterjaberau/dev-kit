{
  "$ref": "#/definitions/TerritoryModel",
  "definitions": {
    "TerritoryModel": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Model Name (e.g. FY24 Planning)"
        },
        "state": {
          "type": "string",
          "enum": [
            "planning",
            "active",
            "archived"
          ],
          "default": "planning"
        },
        "startDate": {
          "type": "string"
        },
        "endDate": {
          "type": "string"
        }
      },
      "required": [
        "name"
      ],
      "additionalProperties": false
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}